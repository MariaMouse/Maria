public class Main {
    public static void main(String[] args) {
                 int[] array = {0, 1, 0, 1, 1, 0};
        for (int i = 0; i < array.length; i++) {
            if (array[i] == 0) {
                array[i] = 1;
            } else {
                array[i] = 0;
            }
        }
        for (int i = 0; i < array.length; i++) {
            System.out.print(array[i] + "");
        }
    }
}


//
public class Main {
    public static void main(String[] args) {
        int[] array = new int[100];
        for (int i = 0; i < 100; i++) {
            array[i] = i + 1;
        }

        for (int i = 0; i < 100; i++) {
            System.out.print(array[i] + " ");
        }
    }
}    
